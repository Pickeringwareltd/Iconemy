<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="author" content="Iconemy">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Iconemy allows your to create your own ICO in less than 10 minutes!">

    <meta property="og:title" content="Iconemy - We make ICOs easy."/>
    <meta property="og:description" content="Create an ICO in less than 10 minutes."/>
    <meta property="og:type" content="website"/>
    <meta property="og:url" content="https://www.iconemy.io"/>
    <meta property="og:image" content="https://www.iconemy.io/images/Iconemy_promo_img.png"/>

    <!-- Fav Icon  -->
    <link rel="shortcut icon" href="/images/Iconemy_just_logo_white_bg.png">
    <!-- Site Title  -->
    <title>Iconemy | ICOs made easy.</title>

    <!-- Fontfaces CSS-->
    <link href="/admin/css/font-face.css" rel="stylesheet" media="all">
    <link href="/admin/vendor/font-awesome-4.7/css/font-awesome.min.css" rel="stylesheet" media="all">
    <link href="/admin/vendor/font-awesome-5/css/fontawesome-all.min.css" rel="stylesheet" media="all">
    <link href="/admin/vendor/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">

    <!-- Bootstrap CSS-->
    <link href="/admin/vendor/bootstrap-4.1/bootstrap.min.css" rel="stylesheet" media="all">

    <!-- Vendor CSS-->
    <link href="/admin/vendor/animsition/animsition.min.css" rel="stylesheet" media="all">
    <link href="/admin/vendor/bootstrap-progressbar/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet" media="all">
    <link href="/admin/vendor/wow/animate.css" rel="stylesheet" media="all">
    <link href="/admin/vendor/css-hamburgers/hamburgers.min.css" rel="stylesheet" media="all">
    <link href="/admin/vendor/slick/slick.css" rel="stylesheet" media="all">
    <link href="/admin/vendor/select2/select2.min.css" rel="stylesheet" media="all">
    <link href="/admin/vendor/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" media="all">

    <!-- Main CSS-->
    <link href="/admin/css/theme.css" rel="stylesheet" media="all">
    <link href="/admin/css/admin_all_projects.css" rel="stylesheet" media="all">
    <link href="/admin/css/project_info.css" rel="stylesheet" media="all">
    <link href="/admin/css/error_message.css" rel="stylesheet" media="all">

    <!-- For uploadcare CDN -->
    <script>
      UPLOADCARE_PUBLIC_KEY = '02b13a5445396427d34c';
    </script>
</head>

<body class="animsition">
    <div class="page-wrapper">
        <!-- HEADER MOBILE-->
        <header class="header-mobile d-block d-lg-none">
            <div class="header-mobile__bar">
                <div class="container-fluid">
                    <div class="header-mobile-inner">
                        <a class="logo" href="index.html">
                            <img src="/images/Iconemy_logo_white_bg.png" alt="Iconemy" />
                        </a>
                        <button class="hamburger hamburger--slider" type="button">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </button>
                    </div>
                </div>
            </div>
            <nav class="navbar-mobile">
                <div class="container-fluid">
                    <ul class="navbar-mobile__list list-unstyled">
                        <li>
                            <a class="js-arrow" src="admin">
                                <i class="fas fa-tachometer-alt"></i>Dashboard</a>
                        </li>
                        <li class="active">
                            <a href="admin/projects">
                                <i class="fas fa-chart-bar"></i>Projects</a>
                        </li>
                        <li>
                            <a href="admin/users">
                                <i class="fas fa-table"></i>Users</a>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <!-- END HEADER MOBILE-->

        <!-- MENU SIDEBAR-->
        <aside class="menu-sidebar d-none d-lg-block">
            <div class="logo">
                <a href="#">
                    <img src="/images/Iconemy_logo_white_bg.png" alt="Iconemy" />
                </a>
            </div>
            <div class="menu-sidebar__content js-scrollbar1">
                <nav class="navbar-sidebar">
                    <ul class="list-unstyled navbar__list">
                        <li class="has-sub">
                            <a class="js-arrow" href="/admin">
                                <i class="fas fa-tachometer-alt"></i>Dashboard
                            </a>
                        </li>
                        <li class="active">
                            <a href="/admin/projects">
                                <i class="fas fa-chart-bar"></i>Projects
                            </a>
                        </li>
                        <li>
                            <a href="table.html">
                                <i class="fas fa-table"></i>Users
                            </a>
                        </li>
                    </ul>
                </nav>
            </div>
        </aside>
        <!-- END MENU SIDEBAR-->

        <!-- PAGE CONTAINER-->
        <div class="page-container">
            <!-- HEADER DESKTOP-->
            <header class="header-desktop">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                    </div>
                </div>
            </header>
            <!-- HEADER DESKTOP-->

            <!-- MAIN CONTENT-->
            <div class="main-content">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-lg-12">
                                <h2 class="title-1 m-b-25"><%= project.name %></h2>
                            </div>
                        </div>
                        
                        <!-- Main statistic board - showing various stats about the project -->
                        <% include partials/main_statistic_board %>

                        <div class="row m-t-25">
                            <div class="col-lg-12">
                                <div class="au-card chart-percent-card">
                                    <div class="au-card-inner">
                                        <h3 class="title-2 tm-b-5">Details</h3>
                                        <div class="row">
                                            <div class="col-md-4">
                                                <div class="menu-sidebar__content js-scrollbar1">
                                                    <nav class="navbar-sidebar">
                                                        <ul class="navbar__list list-unstyled side-menu">
                                                            <li>
                                                                <p class="js-arrow" id="project">
                                                                    <i class="fa fa-th-large"></i>Project</p>
                                                            </li>
                                                            <li>
                                                                <p id="token">
                                                                    <i class="fa fa-dot-circle-o"></i>Token</p>
                                                            </li>

                                                            <!-- Iterate through all crowdsales in the project and create a sub-menu -->
                                                            <% project.crowdsales.forEach(function(sale, i) { %>
                                                                <li>
                                                                    <p class="sale" data-index="<%= sale.index %>">
                                                                        <i class="fa fa-dollar"></i><%= sale.name %></p>
                                                                </li>
                                                            <% }); %>

                                                        </ul>
                                                    </nav>
                                                </div>
                                            </div>
                                            <div class="col-md-8">
                                                <div class="projects">
                                                    <div class="container">
                                                        <div class="row">
                                                            <div class="col-lg-12">

                                                                <h2>Project details</h2>
                                                                
                                                                <!-- Project table -->
                                                                <% include partials/project_details %>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="tokens" style="display: none;">
                                                <% if(project.token){ %>

                                                    <div  class="status" style="background-color: <%= project.token_status.color %>;"><%= project.token_status.message %></div>

                                                    <h2>Token details</h2>

                                                    <!-- Token table -->
                                                    <% include partials/token_details %>

                                                    <!-- If the token has been deployed - show the status of the smart contracts -->

                                                    <% if(project.token.deployed == 'Done'){ %>

                                                        <h3>Contract</h3>

                                                        <!-- Token contract table -->
                                                        <% include partials/token_contract_details %>

                                                    <!-- Else, if the token has been paid for and needs deploying - show the form for setting the smart contracts as deployed -->

                                                    <% } else if (project.token.deployed == 'Deploying') { %>
                                                        
                                                        <h3>Deployment form</h3>

                                                        <!-- Token contract deployment form -->
                                                        <% include partials/token_contract_form %>

                                                    <% } %>

                                                    <!-- If the statuses are either deployed or deploying then the user must have successfully paid for the project, so show the payment details -->

                                                    <% if(project.token.deployed == 'Done' || project.token.deployed == 'Deploying'){ %>

                                                        <h3>Payment</h3>
                                                        
                                                        <!-- Token payment details -->
                                                        <% include partials/token_payment_details %>

                                                    <% } %>
                                                <% } %>
                                                </div>
                                                <% project.crowdsales.forEach(function(sale, i) { %>
                                                    <div class="crowdsales" id="sale_<%= sale.index %>" style="display: none;">
                                                        
                                                        <div  class="status" style="background-color: <%= project.sale_status.color %>"><%= project.sale_status.message %></div>

                                                        <h3><%= sale.name %> details</h3>

                                                        <!-- Sale table -->
                                                        <% include partials/sale_details %>

                                                        <!-- If the sale has been deployed - show the status of the smart contracts -->

                                                        <% if(sale.deployed == 'Done'){ %>

                                                            <h3>Contract</h3>

                                                            <!-- Sale contract table -->
                                                            <% include partials/sale_contract_details %>

                                                        <!-- Else, if the sale has been paid for and needs deploying - show the form for setting the smart contracts as deployed -->

                                                        <% } else if (sale.deployed == 'Deploying') { %>
                                                            
                                                            <h3>Deployment form</h3>

                                                            <!-- Sale contract deployment form -->
                                                            <% include partials/sale_contract_form %>

                                                        <% } %>

                                                        <!-- If the statuses are either deployed or deploying then the user must have successfully paid for the project, so show the payment details -->

                                                        <% if(sale.deployed == 'Done' || sale.deployed == 'Deploying'){ %>

                                                            <h3>Payment</h3>
                                                        
                                                            <!-- Token payment details -->
                                                            <% include partials/sale_payment_details %>

                                                        <% } %>

                                                    </div>
                                                <% }); %>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-12">
                                <div class="copyright">
                                    <p>Copyright © 2018 Colorlib. All rights reserved. Template by <a href="https://colorlib.com">Colorlib</a>.</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Jquery JS-->
    <script src="/admin/vendor/jquery-3.2.1.min.js"></script>

    <% include partials/error_message %>

    <!-- Bootstrap JS-->
    <script src="/admin/vendor/bootstrap-4.1/popper.min.js"></script>
    <script src="/admin/vendor/bootstrap-4.1/bootstrap.min.js"></script>
    <!-- Vendor JS       -->
    <script src="/admin/vendor/slick/slick.min.js"></script>
    <script src="/admin/vendor/wow/wow.min.js"></script>
    <script src="/admin/vendor/animsition/animsition.min.js"></script>
    <script src="/admin/vendor/bootstrap-progressbar/bootstrap-progressbar.min.js"></script>
    <script src="/admin/vendor/counter-up/jquery.waypoints.min.js"></script>
    <script src="/admin/vendor/counter-up/jquery.counterup.min.js"></script>
    <script src="/admin/vendor/circle-progress/circle-progress.min.js"></script>
    <script src="/admin/vendor/perfect-scrollbar/perfect-scrollbar.js"></script>
    <script src="/admin/vendor/chartjs/Chart.bundle.min.js"></script>
    <script src="/admin/vendor/select2/select2.min.js"></script>

    <!-- For image processing via uploadcare -->
    <script src="https://ucarecdn.com/libs/widget/3.5.1/uploadcare.min.js" charset="utf-8"></script>

    <!-- Main JS-->
    <script src="/admin/js/main.js"></script>
    <script src="/admin/js/analytics.js"></script>
    <script src="/admin/js/project_data.js"></script>

</body>

</html>
<!-- end document-->
