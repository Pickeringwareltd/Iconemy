                        <div class="row">
                            <div class="col-lg-6">
                                <div class="au-card au-card--no-shadow au-card--no-pad m-b-40">
                                    <div class="au-card-title" style="background-image:url('images/bg-title-01.jpg');">
                                        <div class="bg-overlay bg-overlay--blue"></div>
                                        <h3>
                                            <i class="zmdi zmdi-account-calendar"></i>Subscribers</h3>
                                    </div>
                                    <div class="au-task js-list-load">
                                        <div class="au-task__title">
                                             <p>There has been
                                                    <span><b><%= subscribed_today %></b></span>

                                                    subscriptions today
                                                </p>
                                        </div>
                                        <div class="au-task-list js-scrollbar3">
                                            <% subscriptions.forEach(function(subscription, i) { %>
                                                <div class="au-task__item au-task__item--primary">
                                                    <div class="au-task__item-inner">
                                                        <h5 class="task">
                                                            <a href="#"><%= subscription.email %></a>
                                                        </h5>
                                                        <span class="time"><%= subscription.time %></span>
                                                    </div>
                                                </div>
                                            <% }); %>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="au-card au-card--no-shadow au-card--no-pad m-b-40">
                                    <div class="au-card-title" style="background-image:url('images/bg-title-02.jpg');">
                                        <div class="bg-overlay bg-overlay--blue"></div>
                                        <h3 id="new_messages_title"><i class="zmdi zmdi-comment-text"></i><a href="/admin">New Messages</a></h3>
                                    </div>
                                    <div class="au-inbox-wrap js-inbox-wrap">
                                        <div class="au-message js-list-load">
                                            <div class="au-message__noti">
                                                <p>You have
                                                    <span><%= unread %></span>

                                                    new messages
                                                </p>
                                            </div>
                                            <div class="au-message-list">

                                                <% messages.forEach(function(message, i) { %>
                                                    <% if(message.responded){ %>
                                                        <div class="au-message__item" data-id="<%= message._id %>" data-email="<%= message.email %>" data-responded="true">
                                                            <div class="au-message__item-inner">
                                                                <div class="au-message__item-text">
                                                                    <div class="text">
                                                                        <h5 class="name"><%= message.name %></h5>
                                                                        <p><%= message.message %></p>
                                                                    </div>
                                                                </div>
                                                                <div class="au-message__item-time">
                                                                    <span><%= message.time %></span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    <% } else { %>
                                                        <div class="au-message__item unread" data-id="<%= message._id %>" data-email="<%= message.email %>" data-responded="false">
                                                            <div class="au-message__item-inner">
                                                                <div class="au-message__item-text">
                                                                    <div class="text">
                                                                        <h5 class="name"><%= message.name %></h5>
                                                                        <p><%= message.message %></p>
                                                                    </div>
                                                                </div>
                                                                <div class="au-message__item-time">
                                                                    <span><%= message.time %></span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    <% } %>

                                                <% }); %>
                                            </div>
                                        </div>
                                        <div class="au-chat">
                                            <div class="au-chat__title">
                                                <div class="au-chat-info">
                                                    <span class="nick">
                                                        <a href="#" id="js_chat_name">John Smith</a>
                                                    </span>
                                                </div>
                                            </div>
                                            <div class="au-chat__title">
                                                <div class="au-chat-info">
                                                    <span class="email">
                                                        <a href="#" id="js_chat_email">john.smith@hotmail.com</a>
                                                    </span>
                                                </div>
                                            </div>
                                            <div class="au-chat__content">
                                                <div class="recei-mess-wrap">
                                                    <span class="mess-time" id="js_chat_time">12 Min ago</span>
                                                    <div class="recei-mess__inner">
                                                        <div class="recei-mess-list">
                                                            <div class="recei-mess" id="js_chat_text">Lorem ipsum dolor sit amet, consectetur adipiscing elit non iaculis</div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            <form class="au-form-icon" id="message_respond_form">
                                                <div class="au-task__footer">
                                                    <button type="submit" class="au-btn au-btn-load js-load-btn">Mark as responded</button>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-6">
                                <div class="au-card recent-report">
                                    <div class="au-card-inner">
                                        <h3 class="title-2">recent reports</h3>
                                        <div class="chart-info">
                                            <div class="chart-info__left">
                                                <div class="chart-note">
                                                    <span class="dot dot--blue"></span>
                                                    <span>products</span>
                                                </div>
                                                <div class="chart-note mr-0">
                                                    <span class="dot dot--green"></span>
                                                    <span>services</span>
                                                </div>
                                            </div>
                                            <div class="chart-info__right">
                                                <div class="chart-statis">
                                                    <span class="index incre">
                                                        <i class="zmdi zmdi-long-arrow-up"></i>25%</span>
                                                    <span class="label">products</span>
                                                </div>
                                                <div class="chart-statis mr-0">
                                                    <span class="index decre">
                                                        <i class="zmdi zmdi-long-arrow-down"></i>10%</span>
                                                    <span class="label">services</span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="recent-report__chart">
                                            <canvas id="recent-rep-chart"></canvas>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="au-card chart-percent-card">
                                    <div class="au-card-inner">
                                        <h3 class="title-2 tm-b-5">Currency by %</h3>
                                        <div class="row no-gutters">
                                            <div class="col-xl-6">
                                                <div class="chart-note-wrap">
                                                    <div class="chart-note mr-0 d-block">
                                                        <span class="dot dot--blue"></span>
                                                        <span>ETH</span>
                                                    </div>
                                                    <div class="chart-note mr-0 d-block">
                                                        <span class="dot dot--red"></span>
                                                        <span>BTC</span>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-xl-6">
                                                <div class="percent-chart">
                                                    <canvas id="earnings_by_currency"></canvas>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
