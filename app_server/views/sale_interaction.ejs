<!DOCTYPE html>
<html lang="zxx" class="js">
<head>

	<!-- Header -->
	<% include partials/header %>

	<!-- Vendor Bundle CSS -->
	<link rel="stylesheet" href="stylesheets/vendor.bundle.css?ver=122">

	<!-- Custom styles for this template -->
	<link rel="stylesheet" href="stylesheets/style.css?ver=122">
	<link rel="stylesheet" href="stylesheets/theme.css?ver=122">
	<link rel="stylesheet" href="stylesheets/custom_styles.css?ver=122">
	<link rel="stylesheet" href="stylesheets/custom_sale.css?ver=122">

</head>

<body class="theme-muscari io-mascari" data-spy="scroll" data-target="#mainnav" data-offset="80">

	<!-- Header --> 
	<header class="site-header is-sticky">

		<!-- Navbar -->
		<div class="navbar navbar-expand-lg is-transparent" id="mainnav">
			<nav class="container">
			
				<a class="navbar-brand animated" data-animate="fadeInDown" data-delay=".65" href="./">
					<img class="logo logo-dark" alt="logo" src="/images/logo_dark_custom.png">
					<img class="logo logo-light" alt="logo" src="/images/logo_light_custom.png">
				</a>
				
<!-- 				<div class="language-switcher animated" data-animate="fadeInDown" data-delay=".75">
					<a href="#" data-toggle="dropdown">EN</a>
					<div class="dropdown-menu" >
						<a class="dropdown-item" href="#">FR</a>
						<a class="dropdown-item" href="#">CH</a>
						<a class="dropdown-item" href="#">BR</a>
					</div>
				</div> -->
				
				<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarToggle">
					<span class="navbar-toggler-icon">
						<span class="ti ti-align-justify"></span>
					</span>
				</button>

				<div class="collapse navbar-collapse justify-content-end" id="navbarToggle">
					<ul class="navbar-nav animated remove-animation" data-animate="fadeInDown" data-delay=".9">
						<li class="nav-item"><a class="nav-link menu-link" href="/">Home</a></li>
						<li class="nav-item"><a class="nav-link menu-link" href="/projects">Projects</a></li>
						<li class="nav-item"><a class="nav-link menu-link" href="https://docs.iconemy.io">Docs</a></li>
					</ul>
					<ul class="navbar-nav navbar-btns animated remove-animation" data-animate="fadeInDown" data-delay="1.15">
						<li class="nav-item"><a class="nav-link btn btn-sm btn-outline menu-link" href="/">Log out</a></li>
					</ul>
				</div>
			</nav>
		</div>
		<!-- End Navbar -->

		<!-- Banner/Slider -->
		<div id="header" class="banner banner-tokensale d-flex align-items-center">
			<div class="header-angle-bg">
				<div class="overlay-bubble">
					<div class="overlay-bubble-1"></div>
					<div class="overlay-bubble-2"></div>
					<div class="overlay-bubble-3"></div>
					<div class="overlay-bubble-4"></div>
					<div class="overlay-bubble-5"></div>
					<div class="overlay-bubble-6"></div>
				</div>
				<div id="particles-js" class="particles-container particles-js"></div>
			</div>
			<div class="container">
				<div class="banner-content">
					<div class="gaps size-3x"></div>
					<div class="row align-items-center mobile-center">
						<div class="col-lg-12 text-center res-m-bttm">
							<div class="header-txt">
								<img src="<%= sale.logo %>" class="center_logo">
								<h1 class="animated token_name" data-animate="fadeInUp" data-delay="1.25"><%= sale.name %></h1>
								<p class="lead animated token_symbol" data-animate="fadeInUp" data-delay="1.35"><%= sale.symbol %></p>
							</div>
						</div><!-- .col  -->
					</div><!-- .row  -->
				</div><!-- .banner-content  -->
				<div class="row justify-content-center text-center sale-box">
					<div class="col-lg-8">
						<span class="warnning-badge large">Pre-sale</span>
						<div class="gaps"></div>
						<div class="token-box shadow">
							<h6 class="small-text">Sale ends in:</h6>
							<div class="token-countdown d-flex align-content-stretch" data-date="2018/09/05"></div>
							<ul class="token-details">
								<li>0 DON</li>
								<li id="center_detail">1 ETH = 50 DON, BTC = 250 DON</li>
								<li>50000 DON</li>
							</ul>
							<div class="token-bar">
								<div class="token-percent" style="width:40%;">16899.49 DON</div>
								<span class="token-point token-point-1" style="left:25%;">Soft Cap</span>
								<span class="token-point token-point-2" style="left:95%;">Hard Cap</span>
							</div>
							<ul class="btns">
								<li><a href="#buy_now" id="buy_btn" class="btn btn-alt btn-lg">BUY NOW</a></li>
							</ul>
							
							<ul class="social">
								<li class="animated" data-animate="fadeInUp" data-delay=".1"><a href="<%= sale.social.facebook %>"><em class="fa fa-facebook"></em></a></li>
								<li class="animated" data-animate="fadeInUp" data-delay=".2"><a href="<%= sale.social.twitter %>"><em class="fa fa-twitter"></em></a></li>
								<li class="animated" data-animate="fadeInUp" data-delay=".3"><a href="<%= sale.social.youtube %>"><em class="fa fa-youtube-play"></em></a></li>
								<li class="animated" data-animate="fadeInUp" data-delay=".4"><a href="<%= sale.social.github %>"><em class="fa fa-github"></em></a></li>
								<li class="animated" data-animate="fadeInUp" data-delay=".5"><a href="<%= sale.social.bitcointalk %>"><em class="fa fa-bitcoin"></em></a></li>
								<li class="animated" data-animate="fadeInUp" data-delay=".6"><a href="<%= sale.social.medium %>"><em class="fa fa-medium"></em></a></li>
							</ul>
						</div>
					</div>
				</div>
			</div><!-- .container  -->
		</div>
	</header>
	<!-- End Header -->
	
	<!-- Start Section -->
	<div class="section section-bg-light section-pad no-pt into-section" id="intro">
		<div class="container">
			<div class="row align-items-center">
				<div class="col-lg-6 col-sm-12">
					<div class="card animated" data-animate="fadeInUp" data-delay=".2">
					  <div class="container token-price">
					  	<p class="title">Token price</p>
					    <h4 class="eth"><span><b id="token_price">0.0014</b></span> ETH</h4>  
					  </div>
					</div>
				</div><!-- .col  -->
				<div class="col-lg-6 col-sm-12">
					<div class="card animated" data-animate="fadeInUp" data-delay=".4">
					  <div class="container">
					  	<p class="title">Total supply</p> 
					    <h4><b>25,123,456</b></h4> 
					  </div>
					</div>
				</div><!-- .col  -->
			</div><!-- .row  -->
			<div class="gaps size-3x"></div>
			<div class="row align-items-center">
				<div class="col-lg-12">
					<div class="card_large" id="buy_now">
					  <div class="container">
					  	<div class="balance">
					  		<p class="title">Balance</p>
					    	<h4 class="token_balance"><b>1,345</b> DON</h4>
					    </div> 
					    <div class="status">
					    	<p class="title">Can buy</p>
					    	<div class="status_dot"></div>
					    </div>
					    <div class="input_fields">
					    	<label for="tokens">Tokens</label>
							<input type="number" id="tokens" name="tokens" placeholder="1250">

							<label for="eth">ETH</label>
							<input type="number" id="eth" name="eth" placeholder="0.0014">
					    </div>
					    <ul class="transfer_btn">
					    	<img src="images/loading_gif.gif" alt="loading" id="loading_gif">
					    	<img src="images/tick_img.png" alt="loading" id="complete_img">
							<li><a id="buynow_btn" class="btn btn-outline">Buy now</a></li>
						</ul>
					  </div>
					</div>
				</div><!-- .col  -->
			</div><!-- .row  -->
			<div class="row align-items-center">
				<div class="col-lg-12">
					<div class="card" id="withdraw">
					  <div class="container">
					  	<div class="balance">
					  		<p class="title">Withdraw</p>
					    </div> 
					    <div class="status">
					    	<p class="title">Can withdraw</p>
					    	<div class="status_dot"></div>
					    </div>
					    <ul class="withdraw_btn">
					    	<img src="images/loading_gif.gif" alt="loading" id="withdraw_loading_gif">
					    	<img src="images/tick_img.png" alt="loading" id="withdraw_complete_img">
							<li><a id="withdraw_btn" class="btn btn-outline">Withdraw</a></li>
						</ul>
					  </div>
					</div>
				</div><!-- .col  -->
			</div><!-- .row  -->
			<div class="row align-items-center">
				<div class="col-lg-12">
					<div class="card" id="stop">
					  <div class="container">
					  	<div class="balance">
					  		<p class="title">Stop sale</p>
					    </div> 
					    <div class="status">
					    	<p class="title">Can stop</p>
					    	<div class="status_dot"></div>
					    </div>
					    <ul class="withdraw_btn">
					    	<img src="images/loading_gif.gif" alt="loading" id="stop_loading_gif">
					    	<img src="images/tick_img.png" alt="loading" id="stop_complete_img">
							<li><a id="stop_btn" class="btn btn-outline">Stop sale</a></li>
						</ul>
					  </div>
					</div>
				</div><!-- .col  -->
			</div><!-- .row  -->
			<div class="gaps size-6x"></div>
			<div class="gaps size-6x"></div>

			<div class="gaps size-3x d-none d-lg-block"></div>
		</div><!-- .conatiner  -->
	</div>
	<!-- End Section -->

	<!-- Footer -->
	<% include partials/footer %>

	<!-- Pre-loader -->
	<% include partials/preloader %>

	<!-- JavaScript (include all script here) -->
	<script src="javascripts/jquery.bundle.js?ver=122"></script>
	<script src="javascripts/script_sale.js?ver=122"></script>
	<script src="javascripts/custom_sale_script.js?ver=122"></script>

</body>
</html>
