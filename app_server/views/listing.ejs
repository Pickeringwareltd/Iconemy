		<!-- Banner -->
		<div id="header" class="page-banner d-flex align-items-center banner-light banner-curb banner-particle just-curve">
			<div class="container">
				<div class="row">
					<div class="col-12">
						<div class="page-head">
							<h2 class="page-heading">ICO Listing</h2>
						</div>
					</div>
				</div>
			</div><!-- .container  -->
