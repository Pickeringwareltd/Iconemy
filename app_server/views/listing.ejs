		<div class="container">
			<div class="row">
				<div class="col-lg-8">
					<div class="blog-list">
						<div class="row">
							<% listings.forEach(function(listing) { %>             
								<div class="col-lg-6 offset-lg-0 col-sm-6">
									<div class="blog-item">
										<div class="blog-photo"><a href="/campaign/<%= listing.domain %>"><img src="<%= listing.cover_photo %>" alt="ico-photo"></a></div>
										<div class="blog-texts">
											<ul class="blog-meta">
												<li><a href="#">OPEN</a></li>
												<li><a href="#">Investment</a></li>
											</ul>
											<h5 class="blog-title"><a href="/campaign/<%= listing.domain %>"><%= listing.name %></a></h5>
											<p><%= listing.tag_line %></p>
										</div>
									</div>
								</div><!-- .col -->
							<% }) %>
